<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div><%= f.email_field :email, :placeholder => "Email", autofocus: true, :class => "loginFormInput" %></div>
  <div><%= f.password_field :password, :placeholder => "Password", autocomplete: "off", :class => "loginFormInput" %></div>
  <div><%= f.submit "Sign in", :class => "loginFormSubmit" %></div>
<% end %>
